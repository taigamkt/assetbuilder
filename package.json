{
  "name": "taiga-market",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "npm run env -s && ng build --prod",
    "start": "npm run env -s && ng serve --aot --proxy-config proxy.conf.js",
    "lint": "ng lint --type-check && stylelint \"src/**/*.scss\" --syntax scss && htmlhint \"src\" --config .htmlhintrc",
    "test": "ng test",
    "test:ci": "npm run lint -s && ng test --single-run --code-coverage",
    "e2e": "npm run env -s && ng e2e",
    "translations:extract": "ngx-translate-extract --input ./src --output ./src/translations/template.json --format=json --clean -sort --marker extract",
    "docs": "hads ./docs -o",
    "env": "ngx-scripts env npm_package_version",
    "generate": "ng generate"
  },
  "dependencies": {
    "@angular/animations": "^5.1.1",
    "@angular/common": "^5.1.1",
    "@angular/compiler": "^5.1.1",
    "@angular/core": "^5.1.1",
    "@angular/forms": "^5.1.1",
    "@angular/http": "^5.1.1",
    "@angular/platform-browser": "^5.1.1",
    "@angular/platform-browser-dynamic": "^5.1.1",
    "@angular/platform-server": "^5.1.1",
    "@angular/router": "^5.1.1",
    "@angular/service-worker": "^5.1.1",
    "@ng-bootstrap/ng-bootstrap": "^1.0.0-beta.9",
    "@ngx-translate/core": "^9.0.1",
    "bootstrap": "4.0.0",
    "core-js": "^2.5.3",
    "crypto-js": "^3.1.9-1",
    "font-awesome": "^4.7.0",
    "jquery": "^1.9.1",
    "lodash": "^4.17.4",
    "popper.js": "^1.12.9",
    "rxjs": "^5.5.5",
    "solc": "^0.4.19",
    "web3": "^1.0.0-beta.30",
    "zone.js": "github:ethereum/web3.js"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.1",
    "@angular/compiler-cli": "^5.1.1",
    "@angular/language-service": "^4.4.6",
    "@types/jasmine": "^2.8.2",
    "@types/jasminewd2": "^2.0.3",
    "@types/node": "^6.0.94",
    "codelyzer": "^3.2.2",
    "jasmine-core": "^2.8.0",
    "jasmine-spec-reporter": "^4.2.1",
    "karma": "^1.7.1",
    "karma-chrome-launcher": "^2.2.0",
    "karma-cli": "^1.0.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.2.2",
    "protractor-console-plugin": "^0.1.1",
    "truffle-contract": "^3.0.0",
    "ts-node": "^3.3.0",
    "tslint": "^5.8.0",
    "typescript": "~2.4.0",
    "zeppelin-solidity": "^1.6.0",
    "@biesbjerg/ngx-translate-extract": "^2.3.1",
    "@ngx-rocket/scripts": "^2.0.0",
    "@types/lodash": "^4.14.52",
    "hads": "^1.3.4",
    "htmlhint": "^0.9.13",
    "https-proxy-agent": "^2.0.0",
    "karma-junit-reporter": "^1.2.0",
    "puppeteer": "^1.0.0",
    "stylelint": "~8.4.0",
    "stylelint-config-recommended-scss": "~3.0.0",
    "stylelint-config-standard": "~18.0.0",
    "stylelint-scss": "~2.2.0"
  }
}
